# PostgreSQL configuration (connect to external database)
POSTGRES_USER=your_db_user
POSTGRES_PASSWORD=your_db_password
POSTGRES_DB=your_db_name
POSTGRES_HOST=your_postgres_host
POSTGRES_PORT=5432

# Kafka configuration (connect to external Kafka server)
KAFKA_BOOTSTRAP_SERVERS=your_kafka_host:9092

# Proxy URL (if needed for HTTP requests)
PROXY_URL=http://your_proxy_url:port
